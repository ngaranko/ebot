{deps, [
  {boss, ".*", {git, "https://github.com/ChicagoBoss/ChicagoBoss.git"}},

  {boss_form, ".*", {git, "git://github.com/ngaranko/boss_form.git", {branch, "0.2.0"}}},
  {hackney, ".*", {git, "git://github.com/benoitc/hackney.git", {branch, "master"}}},
  {restc, ".*", {git, "git://github.com/kivra/restclient.git", {tag, "0.3.4"}}},

  {idna, "1.2.0"},
  {mimerl, "1.0.2"},
  {certifi, "0.4.0"},
  {metrics, "1.0.1"},
  {ssl_verify_fun, "1.1.0"},
  {jsx,".*",{git,"https://github.com/talentdeficit/jsx","v2.6.1"}},
  {erlsom,".*",{git,"https://github.com/willemdj/erlsom","master"}},
  {mochiweb_util,".*",{git,"https://github.com/kivra/mochiweb_util","master"}}
